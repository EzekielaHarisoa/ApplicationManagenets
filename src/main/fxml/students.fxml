<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="950.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/24.0.1" fx:controller="main.java.controller.StudentController">
    <stylesheets>
        <URL value="@/ressources/css/student.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="24.0" prefWidth="950.0" />
      <TabPane layoutX="7.0" layoutY="24.0" prefHeight="518.0" prefWidth="933.0" stylesheets="@../../ressources/css/student.css" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="List of Student and gloabl serache">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox prefHeight="487.0" prefWidth="933.0" styleClass="mainFxmlClass" stylesheets="@../../ressources/css/home.css">
                           <children>
                              <HBox prefHeight="96.0" prefWidth="933.0" />
                              <HBox alignment="CENTER_LEFT" prefHeight="16.0" prefWidth="933.0" spacing="10.0" styleClass="maVbox">
                                 <children>
                                    <TextField prefHeight="30.0" prefWidth="349.0" styleClass="textField" stylesheets="@../../ressources/css/student.css" text="Search students...">
                                       <HBox.margin>
                                          <Insets left="10.0" />
                                       </HBox.margin>
                                    </TextField>
                                 </children>
                                 <VBox.margin>
                                    <Insets top="5.0" />
                                 </VBox.margin>
                              </HBox>
                              <TableView fixedCellSize="10.0" focusTraversable="false" prefHeight="380.0" prefWidth="933.0" stylesheets="@../../ressources/css/student.css">
                                 <columns>
                                    <TableColumn prefWidth="136.0" text="Nom" />
                                    <TableColumn prefWidth="120.0" text="Prenom" />
                                    <TableColumn prefWidth="118.0" text="Telephone" />
                                    <TableColumn prefWidth="97.0" text="Inscrit le" />
                                    <TableColumn prefWidth="108.0" text="Numero ATT" />
                                    <TableColumn prefWidth="112.0" text="Action" />
                                 </columns>
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                                 <cursor>
                                    <Cursor fx:constant="TEXT" />
                                 </cursor>
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Add new students">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox alignment="TOP_CENTER" prefHeight="483.0" prefWidth="950.0" styleClass="card" stylesheets="@../../ressources/css/dialodAdd.css">
                           <children>
                              <HBox alignment="CENTER" prefHeight="32.0" prefWidth="902.0">
                                 <children>
                                    <Label styleClass="card-title" text="➕ Ajouter un nouvel élève" />
                                 </children>
                              </HBox>
                              <VBox prefHeight="334.0" prefWidth="902.0" spacing="20">
                                 <children>
                                    <HBox spacing="20">
                                       <children>
                                          <VBox spacing="5">
                                             <children>
                                                <Label styleClass="text-gray-700 font-medium" text="Nom" />
                                                <TextField fx:id="txtNom" promptText="Entrer le nom" styleClass="input-field" />
                                             </children>
                                          </VBox>
                                          <VBox spacing="5">
                                             <children>
                                                <Label styleClass="text-gray-700 font-medium" text="Prénom" />
                                                <TextField fx:id="txtPrenom" promptText="Entrer le prénom" styleClass="input-field" />
                                             </children>
                                          </VBox>
                                       </children>
                                    </HBox>
                                    <HBox spacing="20">
                                       <children>
                                          <VBox spacing="5">
                                             <children>
                                                <Label styleClass="text-gray-700 font-medium" text="Téléphone" />
                                                <TextField fx:id="txtTel" promptText="Ex: 034 12 345 67" styleClass="input-field" />
                                             </children>
                                          </VBox>
                                          <VBox spacing="5">
                                             <children>
                                                <Label styleClass="text-gray-700 font-medium" text="Catégorie" />
                                                <JFXComboBox fx:id="comboCategorie" promptText="Choisir une catégorie" styleClass="input-field" />
                                             </children>
                                          </VBox>
                                       </children>
                                    </HBox>
                                    <HBox spacing="20">
                                       <children>
                                          <VBox spacing="5">
                                             <children>
                                                <Label styleClass="text-gray-700 font-medium" text="Type de paiement" />
                                                <JFXComboBox fx:id="comboPaiement" prefHeight="43.0" prefWidth="165.0" promptText="Sélectionner un type" styleClass="input-field" />
                                             </children>
                                          </VBox>
                                          <VBox spacing="5">
                                             <children>
                                                <Label styleClass="text-gray-700 font-medium" text="Date examen code" />
                                                <DatePicker fx:id="dateCode" styleClass="input-field" />
                                             </children>
                                          </VBox>
                                       </children>
                                    </HBox>
                                    <VBox spacing="5">
                                       <children>
                                          <Label styleClass="text-gray-700 font-medium" text="Date examen conduite" />
                                          <DatePicker fx:id="dateConduite" styleClass="input-field" />
                                       </children>
                                    </VBox>
                                 </children>
                              </VBox>
                              <HBox alignment="CENTER_RIGHT" spacing="15">
                                 <children>
                                    <JFXButton prefHeight="30.0" prefWidth="161.0" styleClass="btn-cancel" text="Annuler" />
                                    <JFXButton prefHeight="30.0" prefWidth="145.0" styleClass="btn-save" text="Enregistrer" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
         <styleClass>
            <String fx:value="cardFilter" />
            <String fx:value="table-view" />
         </styleClass>
      </TabPane>
   </children>
</AnchorPane>
